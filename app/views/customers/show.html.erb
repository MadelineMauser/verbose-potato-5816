<h1><%=@customer.name%><h1>
<%@customer_items.each do |item|%>
<h3><%=item.name%><h3><br>
Price: <%=item.price%><br>
Supermarket: <%=item.supermarket_name%>
<%end%>
<%=form_with url:"/customers/#{@customer.id}", method: :patch, local: true do |f|%>
<%=f.label :add_item, 'Add an item:'%>
<%=f.text_field :add_item%>
<%=f.submit 'Add'%>
<%end%>